        "format": "<!-- Inter font -->\n<link rel=\"preconnect\" href=\"https://rsms.me/\">\n<link rel=\"stylesheet\" href=\"https://rsms.me/inter/inter.css\">\n\n<style>\n\n    body {\n        font-family: 'Inter', sans-serif; !important;\n    }\n\n    /* Change theme color */\n    body.nr-dashboard-theme {\n        background-color: #212528;\n    }\n\n    /* Toolbar */\n    body.nr-dashboard-theme md-toolbar {\n        background-color: #213770 !important;\n      \n    }\n    \n    /* Toolbar title */\n    .md-toolbar-tools h1 {\n        font-weight: bold !important;\n        text-transform: uppercase !important;\n    }\n\n    /*Groups container*/\n\n    .layout-row>.flex {\n        max-width: 1000px !important;\n        margin: auto !important;\n    }\n\n    .masonry-container {\n        margin-top: 32px !important;\n        max-width: 1000px !important;\n    }\n\n    /*\n    .masonry-container > *{\n        position: static !important;\n        display: flex !important;\n        flex-wrap: wrap !important;\n        justify-content: center !important;\n    }*/\n\n\n    /*Groups*/\n    .nr-dashboard-theme ui-card-panel {\n        background-color: transparent !important;\n        border: 0 !important;\n    }\n\n    .nr-dashboard-theme .nr-dashboard-template {\n        background-color: transparent;\n        padding: 0 !important;\n        border-radius:15px !important;\n        //background: red! important;\n        height: 200px !important;\n    }\n    \n    /*Groups title*/\n    .nr-dashboard-theme ui-card-panel p.nr-dashboard-cardtitle{\n        position: absolute !important;\n        z-index: 1 !important;\n        margin-top: 13px !important;\n        margin-left: 13px !important;\n        font-size: 18.72px !important;\n        color: #ffffff !important;\n        font-weight: bold !important;\n    }\n\n    /*Custom card style*/\n    .custom_card {\n        //background-color: blue !important;\n        height: 190px !important;\n        border-radius: 15px !important;\n        padding: 10px !important;\n    }\n    \n    /* Custom card value */\n    .custom_card h1{\n        margin-left:7px !important;\n        margin-top: 35px !important;\n        color: #ffffff !important;\n        background-color: transparent !important;\n        font-size: 35px !important;\n    }\n\n\n    #Smart_City_Parking_cards{\n        width: 966px !important;\n        height: 410px !important;\n    }\n\n\n    #Smart_City_Parking_cards .nr-dashboard-template{\n        width: 966px !important;\n        height: 410px !important;\n    }\n\n\n\n    /* On screens that are [(318x3) + (6x4)- 1]px or less */\n    @media screen and (max-width: 977px) {\n        \n        #Smart_City_Parking_cards {\n            width: 642px !important;\n        }\n    \n        #Smart_City_Parking_cards .nr-dashboard-template{\n            width: 642px !important;\n        }\n\n        /*Center group flood management*/\n            \n        #Smart_City_Flood_management{\n            left: 50% !important;\n            transform: translate(-50%, 0%) !important;\n        }\n    }\n\n    @media screen and (max-width: 653px) {\n        #Smart_City_Parking_cards{\n             width: 318px !important;\n        }\n        #Smart_City_Parking_cards .nr-dashboard-template{\n            width: 318px !important;\n        }\n    }\n</style>",
